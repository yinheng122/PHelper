page{
    position: relative;
    height: 100%;
}

.canvasView{
  width: 100%;
  height: 100%;
  position: relative;
}

.canvasArea{
  width: 90%;
  height: 100%;
  margin-left: 10%;
  background-color: #FFF;
  z-index: 1;
}

.myCanvas{
  width: 100%;
  height: 100%;
}

.palette{
  position: absolute;
  left: 0;
  bottom: 0;
  top: 0;
  width: 10%;
  height: 100%;
  background-color: #707070;
  z-index: 2;
  /* width: 10%;
  height: 100%;
  background-color: #707070;
  box-shadow:0px 0px 1px 1px lightgray; */
}

.cell{
  width: 100%;
  height: 10vw;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cellImage{
  width: 80%;
  height: 80%;
}
.cellImageS{
  width: 65%;
  height: 65%;
}
.cellColor{
  width: 100%;
  height: 100%;
}

.savePaint{
  width: 50px;
  height: 50px;
  position: absolute;
  bottom: 30px;
  right: 15px;
  background-color: white;
  border-radius: 25px;
}